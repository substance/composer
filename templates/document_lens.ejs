<div class="outline">
  <% _.each(items, function(item, i) { %>
    <div class="outline-item<%= (i>= bounds[0] && i<=bounds[1] ? " active" : "")%><%= selectedItem === i ? " selected" : ""%>"></div>
  <% }); %>
</div>

<div class="items">
  <% _.each(items, function(item, i) { %>
    <div class="item level-<%= item.level %><%= selectedItem === i ? " selected" : ""%>"><a title="<%= item.name %>" data-index="<%= i %>" href="#<%= item.html_id %>"><%= item.name ? item.name.substr(0, 35) : 'Â« Enter Section Name Â»' %></a></div>
  <% }); %>
</div>